generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Beverage {
  id        Int      @id @default(autoincrement())
  name      String
  createdAt DateTime @default(now()) @db.Date
  description String @default("")
}
